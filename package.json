{
  "name": "giganto",
  "version": "1.0.15",
  "description": "Giganto Lib",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/softcorplinux/giganto.git"
  },
  "keywords": [
    "giganto",
    "library",
    "modules",
    "util",
    "javascript",
    "typescript",
    "collection",
    "utilities",
    "objects",
    "functions",
    "arrays"
  ],
  "author": "Isaikin Victor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/softcorplinux/giganto/issues"
  },
  "homepage": "https://github.com/softcorplinux/giganto#readme",
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/jest": "^29.0.3",
    "assert": "^2.0.0",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.3"
  },
  "files": [
    "build/**/*"
  ],
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "jestconfig.json"
      }
    }
  },
  "dependencies": {
    "tslib": "^2.4.1"
  }
}
